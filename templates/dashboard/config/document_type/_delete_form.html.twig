<form method="post" action="{{ path('app_dashboard_config_documentType_delete', {'id': document_type.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ document_type.id) }}">
    <button class="btn btn-phoenix-primary">Delete</button>
</form>
