{% extends 'base.html.twig' %}

{% block title %}WebMenuItem{% endblock %}

{% block body %}
    <h1>WebMenuItem</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ web_menu_item.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ web_menu_item.name }}</td>
            </tr>
            <tr>
                <th>Url</th>
                <td>{{ web_menu_item.url }}</td>
            </tr>
            <tr>
                <th>SortOrder</th>
                <td>{{ web_menu_item.sortOrder }}</td>
            </tr>
            <tr>
                <th>IsActive</th>
                <td>{{ web_menu_item.isActive ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_dashboard_webmanagement_menu_item_index') }}">back to list</a>

    <a href="{{ path('app_dashboard_webmanagement_menu_item_edit', {'id': web_menu_item.id}) }}">edit</a>

    {{ include('dashboard/webmanagement/web_menu_item/_delete_form.html.twig') }}
{% endblock %}
